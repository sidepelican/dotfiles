{
  "title": "mousebutton4, 5",
  "rules": [
    {
      "description": "マウスの戻る・進むボタンをmacでマトモに動くようにする",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4",
            "modifiers": {
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "command"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5",
            "modifiers": {
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    }
  ]
}